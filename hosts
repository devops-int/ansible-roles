[web]
ec2-instance ansible_host=* ansible_user=ec2-user  ansible_ssh_private_key_file=Pair.pem

[mysql-master] 
ec2-instance ansible_host=* ansible_user=ec2-user  ansible_ssh_private_key_file=Pair.pem

[mysql-slave]
ec2-instance2 ansible_host=* ansible_user=ec2-user  ansible_ssh_private_key_file=Pair.pem